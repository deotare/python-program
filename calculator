a=int(input("enter first number:"))
b=int(input("enter second number:"))

print("Please select operation -\n"
      "1. Add\n"
      "2. Subtract\n"  
      "3. Multiply\n" 
      "4. Divide\n")
select=input()

if select == '1': 
    print(a+b)  
elif select == '2': 
    print(a-b) 
elif select == '3': 
    print(a*b) 
elif select == '4': 
    print(a/ b) 
else: 
    print("Invalid input")
